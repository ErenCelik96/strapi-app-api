{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "minLength": 3
    },
    "description": {
      "type": "text",
      "required": true
    },
    "imageUri": {
      "type": "string",
      "required": false
    },
    "userId": {
      "required": false,
      "type": "integer"
    },
    "subject": {
      "type": "string",
      "required": true
    },
    "tutor": {
      "type": "string",
      "required": true
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "post"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "posts"
    }
  }
}
